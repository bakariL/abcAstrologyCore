@model CustomerViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Default view showing</title>
</head>
<body>
    <h4 class="card-header"> New Users </h4>
    <div class="card-body">
        <table>
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Active</th>
                    <th>Date</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model.Customers)
                {
                    <tr>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.IsActive</td>
                        <td>@user.DateRegistered</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    </div>
    </div>
</body>
</html>
